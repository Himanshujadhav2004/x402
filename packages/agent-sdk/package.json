{
  "name": "atomx-agent-sdk",
  "version": "1.0.0",
  "description": "SDK for AI agents to interact with x402 payment-gated APIs on Cronos",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "x402",
    "cronos",
    "ai-agent",
    "payment",
    "api-gateway",
    "eip-3009"
  ],
  "author": "ATOMX Team",
  "license": "MIT",
  "dependencies": {
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "viem": ">=2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atomx/agent-sdk"
  },
  "engines": {
    "node": ">=18"
  }
}
